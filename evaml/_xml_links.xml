<evaml id="229a5dcfde007dda797f986089963ef1" name="script10">
	<settings>
		<voice key="1000" tone="en-US_AllisonV3Voice" />
		<light-effects mode="on" />
		<audio-effects mode="on" vol="100%" />
	</settings>
	<script>
		<random key="1001" max="1" min="1" />
		<light color="blue" key="1002" state="on" />
		<talk key="1003">Hello, what is your name</talk>
		<listen key="1004" />
		<light color="yellow" key="1005" state="on" />
		<evaEmotion emotion="happy" key="1006" />
		<talk key="1007">Hi $, nice to meet you / Hi $, what's up? / It's good to have you $</talk>
		<light color="green" key="1008" state="on" />
		<evaEmotion emotion="neutral" key="1009" />
		<talk key="1010">How are you feeling?</talk>
		<userEmotion key="1011" />
		<light color="white" key="1012" state="on" />
		<switch var="$">
			<case key="1013" op="exact" value="angry" var="$">
				<evaEmotion emotion="angry" key="1014" />
				<talk key="1015">Hey $-1, I see that you are $. Stay calm</talk>
			</case>
			<case key="1016" op="exact" value="sad" var="$">
				<evaEmotion emotion="sad" key="1017" />
				<talk key="1018">Ow no, $-1, I see that you are $. Cheer up</talk>
			</case>
			<case key="1019" op="exact" value="happy" var="$">
				<evaEmotion emotion="happy" key="1020" />
				<talk key="1021">Very good $-1. I will play a song</talk>n
				<audio block="true" key="1022" source="song-mj-preview-small" />
			</case>
			<default key="1023" op="exact" value="" var="$">
				<evaEmotion emotion="sad" key="1024" />
				<talk key="1025">I couldn't identify how you feel</talk>
			</default>
		</switch>
		<evaEmotion emotion="neutral" key="1026" />
		<talk key="1027">Bye bye</talk>
		<light key="1028" state="off" />
	</script>
	<links><link from="1000" to="1001" /><link from="1001" to="1002" /><link from="1002" to="1003" /><link from="1003" to="1004" /><link from="1004" to="1005" /><link from="1005" to="1006" /><link from="1006" to="1007" /><link from="1007" to="1008" /><link from="1008" to="1009" /><link from="1009" to="1010" /><link from="1010" to="1011" /><link from="1011" to="1012" /><link from="1012" to="1013" /><link from="1013" to="1014" /><link from="1014" to="1015" /><link from="1015" to="1026" /><link from="1012" to="1016" /><link from="1016" to="1017" /><link from="1017" to="1018" /><link from="1018" to="1026" /><link from="1012" to="1019" /><link from="1019" to="1020" /><link from="1020" to="1021" /><link from="1021" to="1022" /><link from="1022" to="1026" /><link from="1012" to="1023" /><link from="1023" to="1024" /><link from="1024" to="1025" /><link from="1025" to="1026" /><link from="1026" to="1027" /><link from="1027" to="1028" /></links></evaml>